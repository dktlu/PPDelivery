<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape>
            <corners android:topRightRadius="@dimen/x38" android:bottomRightRadius="@dimen/x38"/>
            <solid android:color="@color/white"/>
            <stroke android:color="@color/login_blue_1A" android:width="@dimen/x2"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape>
            <corners android:topRightRadius="@dimen/x38" android:bottomRightRadius="@dimen/x38"/>
            <solid android:color="@color/login_blue_1A"/>
        </shape>
    </item>
</selector>