<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:orientation="vertical">

    <include
        android:id="@+id/titlebar"
        layout="@layout/layout_titlebar"/>

    <View
        style="@style/divider_height_1"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content">

            <!--提现金额-->
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:background="@color/login_blue_1A"
                android:layout_height="@dimen/x160">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:orientation="horizontal"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/ww_txt_color_white_size_24"
                            android:text="¥"/>

                        <TextView
                            style="@style/ww_txt_color_white"
                            android:textSize="@dimen/x70"
                            android:text="200"/>

                    </LinearLayout>

                    <TextView
                        style="@style/ww_txt_color_white_size_24"
                        android:text="@string/total_commission"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:orientation="horizontal"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/ww_txt_color_white_size_24"
                            android:text="¥"/>

                        <TextView
                            style="@style/ww_txt_color_white"
                            android:textSize="@dimen/x70"
                            android:text="100"/>

                    </LinearLayout>

                    <TextView
                        style="@style/ww_txt_color_white_size_24"
                        android:text="@string/already_withdrawals"/>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                style="@style/ll_hor_gravity_center_vert"
                android:paddingLeft="@dimen/x30"
                android:paddingRight="@dimen/x30"
                android:layout_height="@dimen/x90">

                <TextView
                    style="@style/ww_txt_color_gray45_size_28"
                    android:text="@string/name"/>

                <EditText
                    style="@style/ww_txt_color_gray45_size_28_gravity_right"
                    android:layout_weight="1"
                    android:background="@color/white"/>

            </LinearLayout>

            <View
                style="@style/divider_height_1"/>

            <View
                android:layout_marginTop="@dimen/x20"
                style="@style/divider_height_1"/>

            <!--支付宝提现-->
            <LinearLayout
                style="@style/LinearLayout_mw_vertical_bg_white">

                <RelativeLayout
                    style="@style/mw_margin_left_right_30"
                    android:layout_height="@dimen/x110">

                    <TextView
                        style="@style/ww_txt_color_gray45_size_28"
                        android:drawablePadding="@dimen/x24"
                        android:drawableLeft="@mipmap/ic_withdrawals_zfb"
                        android:layout_alignParentLeft="true"
                        android:gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:text="@string/withdrawals_zfb"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:button="@null"
                        android:background="@drawable/selector_registration"
                        android:layout_alignParentRight="true"/>

                </RelativeLayout>

                <View
                    style="@style/divider_height_1"/>

                <LinearLayout
                    style="@style/ll_hor_gravity_center_vert_margin_left_right_30"
                    android:layout_height="@dimen/x90">

                    <TextView
                        style="@style/ww_txt_color_gray45_size_28"
                        android:text="@string/zfb_name"/>

                    <EditText
                        style="@style/ww_txt_color_gray45_size_28_gravity_right"
                        android:layout_weight="1"
                        android:background="@color/white"/>

                </LinearLayout>

                <View
                    style="@style/divider_height_1_marginLeft_30"/>

                <LinearLayout
                    style="@style/ll_hor_gravity_center_vert_margin_left_right_30"
                    android:layout_height="@dimen/x90">

                    <TextView
                        style="@style/ww_txt_color_gray45_size_28"
                        android:text="@string/zfb_account"/>

                    <EditText
                        style="@style/ww_txt_color_gray45_size_28_gravity_right"
                        android:layout_weight="1"
                        android:background="@color/white"/>

                </LinearLayout>

                <View
                    style="@style/divider_height_1"/>

            </LinearLayout>

            <View
                android:layout_marginTop="@dimen/x20"
                style="@style/divider_height_1"/>

            <!--银行卡提现-->
            <LinearLayout
                style="@style/LinearLayout_mw_vertical_bg_white">

                <RelativeLayout
                    style="@style/mw_margin_left_right_30"
                    android:layout_height="@dimen/x110">

                    <TextView
                        style="@style/ww_txt_color_gray45_size_28"
                        android:drawablePadding="@dimen/x24"
                        android:drawableLeft="@mipmap/ic_withdrawals_bank"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:text="@string/withdrawals_bank"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:button="@null"
                        android:background="@drawable/selector_registration"
                        android:layout_alignParentRight="true"/>

                </RelativeLayout>

                <View
                    style="@style/divider_height_1"/>

                <LinearLayout
                    style="@style/ll_hor_gravity_center_vert_margin_left_right_30"
                    android:layout_height="@dimen/x90">

                    <TextView
                        style="@style/ww_txt_color_gray45_size_28"
                        android:text="@string/bank_name"/>

                    <EditText
                        style="@style/ww_txt_color_gray45_size_28_gravity_right"
                        android:layout_weight="1"
                        android:background="@color/white"/>

                </LinearLayout>

                <View
                    style="@style/divider_height_1_marginLeft_30"/>

                <LinearLayout
                    style="@style/ll_hor_gravity_center_vert_margin_left_right_30"
                    android:layout_height="@dimen/x90">

                    <TextView
                        style="@style/ww_txt_color_gray45_size_28"
                        android:text="@string/bank_address"/>

                    <EditText
                        style="@style/ww_txt_color_gray45_size_28_gravity_right"
                        android:layout_weight="1"
                        android:background="@color/white"/>

                </LinearLayout>

                <View
                    style="@style/divider_height_1_marginLeft_30"/>

                <LinearLayout
                    style="@style/ll_hor_gravity_center_vert_margin_left_right_30"
                    android:layout_height="@dimen/x90">

                    <TextView
                        style="@style/ww_txt_color_gray45_size_28"
                        android:text="@string/bank_account"/>

                    <EditText
                        style="@style/ww_txt_color_gray45_size_28_gravity_right"
                        android:layout_weight="1"
                        android:background="@color/white"/>

                </LinearLayout>

                <View
                    style="@style/divider_height_1"/>
            </LinearLayout>

            <View
                android:layout_marginTop="@dimen/x20"
                style="@style/divider_height_1"/>

            <!--可提现余额-->
            <LinearLayout
                style="@style/LinearLayout_mw_horizontal_bg_white"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/x30"
                android:paddingRight="@dimen/x30"
                android:layout_height="@dimen/x90">

                <TextView
                    style="@style/ww_txt_color_gray45_size_28"
                    android:text="@string/can_withdrawals_balance"/>

                <EditText
                    style="@style/ww_txt_color_gray45_size_28_gravity_right"
                    android:layout_weight="1"
                    android:background="@color/white"/>

            </LinearLayout>

            <View
                style="@style/divider_height_1"/>

            <TextView
                android:layout_marginLeft="@dimen/x30"
                android:layout_marginRight="@dimen/x30"
                android:layout_marginTop="@dimen/x24"
                android:layout_marginBottom="@dimen/x36"
                android:text="@string/withdrawals_tips"
                style="@style/ww_color_gray45_size_24"/>

            <TextView
                style="@style/ww_color_white_size_30"
                android:layout_height="@dimen/x88"
                android:background="@drawable/shape_blue_solid"
                android:gravity="center"
                android:text="@string/withdrawals"
                android:layout_marginBottom="@dimen/x30"
                android:layout_marginLeft="@dimen/x30"
                android:layout_marginRight="@dimen/x30"
                android:layout_width="match_parent"/>

        </LinearLayout>
    </ScrollView>

</LinearLayout>
